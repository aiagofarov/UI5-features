{
	"_version": "1.7.0",
	"sap.app": {
		"id": "UI5-features",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"GWSAMPLE_BASIC": {
				"uri": "/sap/opu/odata/iwbep/GWSAMPLE_BASIC/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "UI5-features.view.App",
			"type": "XML",
			"id": "rootViewId"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "UI5-features.i18n.i18n"
				}
			},
			"MyJSONModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/JSONModelName.json"
			},
			"MyODataModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "GWSAMPLE_BASIC",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "UI5-features.view",
				"controlId": "rootViewId",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "startRoute",
				"target": "startTarget"
			}, {
				"pattern": "secondPageHash",
				"name": "secondRoute",
				"target": "secondTarget"
			}, {
				"pattern": "thirdPageHash",
				"name": "thirdRoute",
				"target": "thirdTarget"
			},{
				"pattern": "fourthPageHash/{entry_id}",
				"name": "fourthRoute",
				"target": "fourthTarget"
			}],
			"targets": {
				"startTarget": {
					"viewId": "start",
					"viewName": "Start",
					"viewLevel": 1
				},
				"secondTarget": {
					"viewId": "second",
					"viewName": "Second",
					"viewLevel": 1
				},
				"thirdTarget": {
					"viewId": "third",
					"viewName": "Third",
					"viewLevel": 1
				},
				"fourthTarget": {
					"viewId": "fourth",
					"viewName": "Fourth",
					"viewLevel": 1
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}